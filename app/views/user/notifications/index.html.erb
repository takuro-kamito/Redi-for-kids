<% notifications = @notifications.where.not(visitor_id: current_user.id) %>
<% if notifications.exists? %>
<%= link_to "全削除",user_notification_path(@notifications), method: :delete,class:"btn btn-light" %>
<% @notifications.each do |notification| %>
<%= render 'user/notifications/notificastion', notifications: notifications %>
<% end %>
<% else %>
    <p>通知はございません</p>
<% end %>