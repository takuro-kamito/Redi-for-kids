<h2> MY Room </h2>
<% @another_entries.each do |entry| %>
<%= link_to user_room_path(entry.room) do %>
<div class="card">
  <div class="card-body">
    <div class="d-flex">
        <div class="chat-user-name">
          <%= image_tag entry.user.get_profile_image(60, 60), class: "rounded-circle mx-auto d-block" %>
          <%= entry.user.name %>
             <div class="chat-text">
            <%= Message.find_by(id: entry.room.message_ids.last)&.body %>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<% end %>
<% end %>
